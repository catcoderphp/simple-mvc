{
  "name": "catcoderphp/simple-mvc-php",
  "description":"Simple mvc php application with eloquent and twig",
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "catcoderphp/simple-mvc",
        "version": "dev-master",
        "source": {
          "url": "https://github.com/catcoderphp/simple-mvc.git",
          "type": "git",
          "reference": "master"
        },
        "autoload": {
          "psr-0" : {
            "catcoderphp/simple-mvc" : "app"
          }
        }
      }
    }
  ],
  "require": {
    "illuminate/database": "4.1.*",
    "twig/twig": "1.*",
    "filp/whoops": "~1.1",
    "guzzlehttp/guzzle": "~6.0"
  },
  "autoload": {
    "classmap": [
      "app/controllers",
      "app/models",
      "app/core"
    ],
    "files": [
      "app/database.php"
    ]
  }
}
